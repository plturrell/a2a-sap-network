{
  "name": "a2a-network",
  "version": "1.0.0",
  "private": true,
  "description": "A2A Network - Enterprise Agent Coordination Platform on SAP BTP",
  "homepage": "https://sap.com/a2a",
  "repository": {
    "type": "git",
    "url": "https://github.com/sap/a2a-network.git"
  },
  "license": "SAP SAMPLE CODE LICENSE",
  "engines": {
    "node": "^18 || ^20"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds watch",
    "build": "rimraf gen && cds build --production",
    "build:cf": "mbt build -p=cf",
    "build:xs-security": "cds compile srv/ --to xsuaa > xs-security.json",
    "deploy": "cf deploy mta_archives/a2a-network_1.0.0.mtar",
    "deploy:local": "cds deploy --to sqlite --no-save",
    "deploy:hana": "cds deploy --to hana",
    "test": "jest --runInBand --forceExit",
    "test:unit": "jest --testPathPattern=unit --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint . --ext .js,.cds && solhint 'src/**/*.sol'",
    "format": "prettier --write \"**/*.{js,json,cds,md}\" && forge fmt",
    "mtx:deploy": "cds build && cf deploy mta_archives/*mtar -e mtx-app-vars.yml",
    "security:audit": "node scripts/security-audit.js"
  },
  "dependencies": {
    "@sap-cloud-sdk/connectivity": "^3.8.0",
    "@sap-cloud-sdk/http-client": "^3.8.0",
    "@sap-cloud-sdk/resilience": "^3.8.0",
    "@sap-cloud-sdk/util": "^3.8.0",
    "@sap/approuter": "^14.4.0",
    "@sap/audit-logging": "^5.7.0",
    "@sap/cds": "^7.5.0",
    "@sap/cds-compiler": "^3.8.0",
    "@sap/cds-dk": "^7.5.0",
    "@sap/cds-foss": "^4.0.0",
    "@sap/cds-mtxs": "^1.12.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/cds-rest": "^1.3.0",
    "@sap/eslint-plugin-cds": "^3.0.0",
    "@sap/hana-client": "^2.19.21",
    "@sap/hdi-deploy": "^4.8.0",
    "@sap/html5-app-deployer": "^4.1.0",
    "@sap/instance-manager": "^3.3.0",
    "@sap/logging": "^7.0.0",
    "@sap/xsenv": "^4.2.0",
    "@sap/xssec": "^3.6.0",
    "@supabase/supabase-js": "^2.38.4",
    "cors": "^2.8.5",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "hdb": "^0.19.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "socket.io": "^4.6.0",
    "uuid": "^9.0.1",
    "web3": "^4.3.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1.4.0",
    "@sap/eslint-plugin-ui5-jsdocs": "^2.0.0",
    "@sap/ux-specification": "^1.108.0",
    "@sap/ux-ui5-tooling": "^1.11.0",
    "@sapui5/ts-types": "^1.120.0",
    "@types/node": "^20.10.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "typescript": "^5.3.0"
  },
  "cds": {
    "requires": {
      "db": {
        "[development]": {
          "kind": "hana-cloud",
          "credentials": {
            "host": "localhost",
            "port": 30015,
            "database": "A2A_DEV"
          }
        },
        "[production]": {
          "kind": "hana-cloud"
        }
      },
      "supabase": {
        "kind": "rest",
        "credentials": {
          "url": "${SUPABASE_URL}",
          "key": "${SUPABASE_ANON_KEY}"
        }
      },
      "auth": {
        "[development]": {
          "kind": "xsuaa"
        },
        "[production]": {
          "kind": "xsuaa"
        }
      },
      "blockchain": {
        "impl": "@a2a/blockchain-service",
        "rpc": "http://localhost:8545",
        "contracts": {
          "AgentRegistry": "./out/AgentRegistry.sol/AgentRegistry.json",
          "MessageRouter": "./out/MessageRouter.sol/MessageRouter.json",
          "AgentServiceMarketplace": "./out/AgentServiceMarketplace.sol/AgentServiceMarketplace.json",
          "CapabilityMatcher": "./out/CapabilityMatcher.sol/CapabilityMatcher.json",
          "PerformanceReputationSystem": "./out/PerformanceReputationSystem.sol/PerformanceReputationSystem.json"
        }
      }
    },
    "hana": {
      "deploy-format": "hdbtable",
      "syntax": "hdi"
    },
    "features": {
      "fiori_preview": true,
      "in_memory_cache": true,
      "serve_static": true
    },
    "build": {
      "tasks": [
        {
          "for": "hana",
          "dest": "../db"
        },
        {
          "for": "node-cf"
        }
      ]
    },
    "i18n": {
      "default_language": "en",
      "languages": [
        "en",
        "de",
        "fr",
        "es",
        "zh",
        "ja"
      ]
    }
  },
  "sapux": [
    "app/a2a-fiori"
  ],
  "jest": {
    "preset": "@sap/cds-jest",
    "testEnvironment": "node",
    "testTimeout": 20000,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "extends": "@sap/eslint-plugin-cds",
    "plugins": [
      "@sap/ui5-jsdocs"
    ],
    "rules": {
      "@sap/ui5-jsdocs/no-jsdoc": "error"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "main": "srv/server.js",
  "keywords": [
    "SAP",
    "CAP",
    "BTP",
    "Cloud",
    "Enterprise",
    "blockchain",
    "agents",
    "A2A",
    "Fiori",
    "HANA"
  ],
  "author": "SAP SE"
}
