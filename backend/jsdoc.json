{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": [
      "app/a2a/developer_portal/static",
      "app/a2a/developer_portal/cap/srv",
      "app/a2a/developer_portal/cap/app",
      "src"
    ],
    "exclude": [
      "node_modules",
      "test",
      "tests",
      "dist",
      "build",
      ".git"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": true,
      "useLongnameInNav": true
    }
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "readme": "./README.md",
    "template": "templates/default",
    "tutorials": "./docs/tutorials"
  },
  "metadata": {
    "title": "A2A Platform API Documentation",
    "description": "Comprehensive API documentation for the A2A Agent Platform"
  }
}