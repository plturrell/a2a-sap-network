{
    "_version": "1.58.0",
    "sap.app": {
        "id": "a2a.portal",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.11.4",
            "toolsId": "b4863db0-b0a9-4f3e-8f7a-9f8c9b8d5e6f"
        },
        "dataSources": {
            "mainService": {
                "uri": "/api/",
                "type": "REST"
            },
            "i18n": {
                "uri": "i18n/i18n.properties",
                "type": "bundle"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "rootView": {
            "viewName": "a2a.portal.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.tnt": {},
                "sap.ui.layout": {},
                "sap.f": {}
            }
        },
        "models": {
            "i18n": {
                "dataSource": "i18n",
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "a2a.portal.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "../../css/developer-portal.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "a2a.portal.view",
                "controlAggregation": "mainContents",
                "controlId": "app",
                "clearControlAggregation": false,
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [
                {
                    "name": "RouteApp",
                    "pattern": ":?query:",
                    "target": ["TargetProjects"]
                },
                {
                    "name": "projects",
                    "pattern": "projects",
                    "target": ["TargetProjects"]
                },
                {
                    "name": "userProfile",
                    "pattern": "profile",
                    "target": ["TargetUserProfile"]
                },
                {
                    "name": "a2aNetwork",
                    "pattern": "a2a-network",
                    "target": ["TargetA2ANetwork"]
                },
                {
                    "name": "templates",
                    "pattern": "templates",
                    "target": ["TargetTemplates"]
                },
                {
                    "name": "testing",
                    "pattern": "testing",
                    "target": ["TargetTesting"]
                },
                {
                    "name": "deployment",
                    "pattern": "deployment",
                    "target": ["TargetDeployment"]
                },
                {
                    "name": "monitoring",
                    "pattern": "monitoring",
                    "target": ["TargetMonitoring"]
                },
                {
                    "name": "projectDetail",
                    "pattern": "project/{projectId}",
                    "target": ["TargetProjectDetail"]
                },
                {
                    "name": "agentBuilder",
                    "pattern": "project/{projectId}/agent-builder",
                    "target": ["TargetAgentBuilder"]
                },
                {
                    "name": "bpmnDesigner",
                    "pattern": "project/{projectId}/bpmn-designer",
                    "target": ["TargetBPMNDesigner"]
                },
                {
                    "name": "codeEditor",
                    "pattern": "project/{projectId}/code-editor",
                    "target": ["TargetCodeEditor"]
                },
                {
                    "name": "projectObjectPage",
                    "pattern": "project/{projectId}/details",
                    "target": ["TargetProjectObjectPage"]
                }
            ],
            "targets": {
                "TargetProjects": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Projects",
                    "viewName": "Projects"
                },
                "TargetProjectDetail": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ProjectDetail",
                    "viewName": "ProjectDetail"
                },
                "TargetAgentBuilder": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "AgentBuilder",
                    "viewName": "AgentBuilder"
                },
                "TargetBPMNDesigner": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "BPMNDesigner",
                    "viewName": "BPMNDesigner"
                },
                "TargetCodeEditor": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "CodeEditor",
                    "viewName": "CodeEditor"
                },
                "TargetUserProfile": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "UserProfile",
                    "viewName": "UserProfile"
                },
                "TargetA2ANetwork": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "A2ANetworkManager",
                    "viewName": "A2ANetworkManager"
                },
                "TargetTemplates": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Templates",
                    "viewName": "Templates"
                },
                "TargetTesting": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Testing",
                    "viewName": "Testing"
                },
                "TargetDeployment": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Deployment",
                    "viewName": "Deployment"
                },
                "TargetMonitoring": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "Monitoring",
                    "viewName": "Monitoring"
                },
                "TargetProjectObjectPage": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ProjectObjectPage",
                    "viewName": "ProjectObjectPage"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "a2a.portal"
    }
}