  "monitoring": {
    "enable_metrics": true,
    "metrics_port": 9090,
    "enable_logging": true,
    "log_level": "WARNING",
    "log_format": "json",
    "enable_tracing": true,
    "jaeger_endpoint": null,
    "enable_health_checks": true,
    "health_check_interval": 30
  },
  "contracts": {
    "agent_registry_address": null,
    "message_router_address": null,
    "ord_registry_address": null,
    "deployment_block": null,
    "enable_upgrades": false,
    "proxy_admin_address": null
  }
