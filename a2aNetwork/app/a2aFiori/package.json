{
  "name": "a2a-network-fiori",
  "version": "1.0.0",
  "description": "A2A Network SAP Fiori Application",
  "private": true,
  "scripts": {
    "start": "ui5 serve --config=ui5.yaml --open index.html",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "ui5 build --config=ui5-deploy.yaml --clean-dest",
    "lint": "eslint webapp",
    "test": "echo \"No tests specified\"",
    "preload": "ui5 build preload --clean-dest --config ui5.yaml --all"
  },
  "dependencies": {
    "ws": "^8.14.2",
    "web-push": "^3.6.6",
    "axios": "^1.6.0",
    "@sap/cds": "^7.0.0"
  },
  "devDependencies": {
    "@ui5/cli": "^3.9.1",
    "@sap/ux-ui5-tooling": "^1.11.0",
    "@ui5/logger": "^3.0.0",
    "ui5-middleware-livereload": "^3.0.2",
    "ui5-task-zipper": "^3.1.1",
    "ui5-task-minifier": "^2.0.0"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling"
    ]
  }
}