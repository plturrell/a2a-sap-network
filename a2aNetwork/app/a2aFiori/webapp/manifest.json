{
    "_version": "1.60.0",
    "sap.app": {
        "id": "a2a.network.fiori",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "ach": "CA-UI5-FE",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:lrop",
            "version": "1.11.0",
            "toolsId": "3ef2e000-b8b2-4b80-85bd-c6d24f30a2a1"
        },
        "dataSources": {
            "mainService": {
                "uri": "/api/v1/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "blockchainService": {
                "uri": "/blockchain/",
                "type": "REST"
            }
        },
        "crossNavigation": {
            "inbounds": {
                "a2a-network-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "A2ANetwork",
                    "action": "manage",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": "sap-icon://collaborate"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://collaborate",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "config": {
            "sapFiori2Adaptation": true
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.ushell": {},
                "sap.fe.templates": {},
                "sap.ui.fl": {},
                "sap.ui.comp": {},
                "sap.ui.table": {},
                "sap.uxap": {},
                "sap.ui.layout": {},
                "sap.viz": {},
                "sap.suite.ui.microchart": {},
                "sap.collaboration": {},
                "sap.ui.unified": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "a2a.network.fiori.i18n.i18n",
                    "supportedLocales": ["en", "de", "fr", "es", "zh", "ja"],
                    "fallbackLocale": "en"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "groupId": "$auto",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/designTokens.css"
                },
                {
                    "uri": "css/sharedComponents.css"
                },
                {
                    "uri": "css/style.css"
                },
                {
                    "uri": "css/loadingStates.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "a2a.network.fiori.view",
                "controlId": "navContainer",
                "controlAggregation": "pages",
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                },
                "async": true
            },
            "routes": [
                {
                    "name": "home",
                    "pattern": "",
                    "target": "home"
                },
                {
                    "name": "agents",
                    "pattern": "agents",
                    "target": "agents"
                },
                {
                    "name": "agentDetail",
                    "pattern": "agents/{agentId}",
                    "target": "agentDetail"
                },
                {
                    "name": "services",
                    "pattern": "services",
                    "target": "services"
                },
                {
                    "name": "capabilities",
                    "pattern": "capabilities",
                    "target": "capabilities"
                },
                {
                    "name": "workflows",
                    "pattern": "workflows",
                    "target": "workflows"
                },
                {
                    "name": "analytics",
                    "pattern": "analytics",
                    "target": "analytics"
                },
                {
                    "name": "blockchain",
                    "pattern": "blockchain",
                    "target": "blockchain"
                },
                {
                    "name": "agentVisualization",
                    "pattern": "agent-visualization",
                    "target": "agentVisualization"
                },
                {
                    "name": "contractDetail",
                    "pattern": "contracts/{address}",
                    "target": "contractDetail"
                },
                {
                    "name": "settings",
                    "pattern": "settings",
                    "target": "settings"
                },
                {
                    "name": "transactions",
                    "pattern": "transactions",
                    "target": "transactions"
                },
                {
                    "name": "operations",
                    "pattern": "operations",
                    "target": "operations"
                },
                {
                    "name": "marketplace",
                    "pattern": "marketplace",
                    "target": "marketplace"
                },
                {
                    "name": "contracts",
                    "pattern": "contracts",
                    "target": "contracts"
                },
                {
                    "name": "logs",
                    "pattern": "logs",
                    "target": "logs"
                },
                {
                    "name": "alerts",
                    "pattern": "alerts",
                    "target": "alerts"
                },
                {
                    "name": "offline",
                    "pattern": "offline",
                    "target": "offline"
                }
            ],
            "targets": {
                "home": {
                    "viewType": "XML",
                    "viewName": "Home",
                    "viewLevel": 1
                },
                "agents": {
                    "viewType": "XML",
                    "viewName": "Agents",
                    "viewLevel": 2
                },
                "agentDetail": {
                    "viewType": "XML",
                    "viewName": "AgentDetail",
                    "viewLevel": 3
                },
                "services": {
                    "viewType": "XML",
                    "viewName": "Services",
                    "viewLevel": 2
                },
                "capabilities": {
                    "viewType": "XML",
                    "viewName": "Capabilities",
                    "viewLevel": 2
                },
                "workflows": {
                    "viewType": "XML",
                    "viewName": "Workflows",
                    "viewLevel": 2
                },
                "analytics": {
                    "viewType": "XML",
                    "viewName": "Analytics",
                    "viewLevel": 2
                },
                "blockchain": {
                    "viewType": "XML",
                    "viewName": "BlockchainDashboard",
                    "viewLevel": 2
                },
                "agentVisualization": {
                    "viewType": "XML",
                    "viewName": "AgentVisualization",
                    "viewLevel": 2
                },
                "contractDetail": {
                    "viewType": "XML",
                    "viewName": "ContractDetail",
                    "viewLevel": 3
                },
                "settings": {
                    "viewType": "XML",
                    "viewName": "Settings",
                    "viewLevel": 2
                },
                "transactions": {
                    "viewType": "XML",
                    "viewName": "Transactions",
                    "viewLevel": 2
                },
                "operations": {
                    "viewType": "XML",
                    "viewName": "Operations",
                    "viewLevel": 2
                },
                "marketplace": {
                    "viewType": "XML",
                    "viewName": "Marketplace",
                    "viewLevel": 2
                },
                "contracts": {
                    "viewType": "XML",
                    "viewName": "Contracts",
                    "viewLevel": 2
                },
                "logs": {
                    "viewType": "XML",
                    "viewName": "Logs",
                    "viewLevel": 2
                },
                "alerts": {
                    "viewType": "XML",
                    "viewName": "Alerts",
                    "viewLevel": 2
                },
                "offline": {
                    "viewType": "XML",
                    "viewName": "Offline",
                    "viewLevel": 2
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": ["F0001"],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "a2a.network.fiori"
    },
    "sap.platform.cf": {
        "ui5VersionNumber": "1.120.0"
    },
    "sap.flp": {
        "type": "application"
    },
    "sap.card": {
        "type": "list",
        "configuration": {
            "destinations": {
                "A2AService": {
                    "name": "A2AService"
                }
            }
        }
    },
    "sap.pwa": {
        "enabled": true,
        "appShortName": "A2A Network",
        "appName": "Agent-to-Agent Network Platform",
        "description": "Blockchain-based autonomous agent orchestration platform",
        "startUrl": "./",
        "scope": "./",
        "display": "standalone",
        "orientation": "portrait-primary",
        "themeColor": "#0070f2",
        "backgroundColor": "#ffffff",
        "icons": [
            {
                "src": "./resources/pwa/icon-192.png",
                "sizes": "192x192",
                "type": "image/png"
            },
            {
                "src": "./resources/pwa/icon-512.png",
                "sizes": "512x512",
                "type": "image/png"
            }
        ]
    }
}