{
    "_version": "1.49.0",
    "sap.app": {
        "id": "a2a.network.agent15.ext",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "AI-WF-ORC",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ORCHESTRATOR_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "i18n": {
                "uri": "i18n/i18n.properties",
                "type": "properties"
            }
        },
        "crossNavigation": {
            "inbounds": {
                "orchestratorAgent": {
                    "semanticObject": "OrchestratorAgent",
                    "action": "manage",
                    "title": "{{appTitle}}",
                    "subTitle": "{{appDescription}}"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.uxap": {},
                "sap.f": {},
                "sap.ui.layout": {},
                "sap.suite.ui.commons": {},
                "sap.viz": {}
            }
        },
        "models": {
            "i18n": {
                "dataSource": "i18n",
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "a2a.network.agent15.ext.i18n.i18n",
                    "supportedLocales": [""],
                    "fallbackLocale": ""
                }
            },
            "": {
                "dataSource": "mainService",
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true,
                    "preliminaryContext": true,
                    "headers": {
                        "X-Requested-With": "XMLHttpRequest"
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "flexEnabled": true,
        "handleValidation": true
    },
    "sap.fiori": {
        "registrationIds": ["F7015"],
        "archeType": "transactional"
    },
    "sap.ui.generic.app": {
        "_version": "1.3.0",
        "settings": {
            "forceGlobalRefresh": false,
            "objectPageHeaderType": "Dynamic",
            "considerAnalyticalParameters": true,
            "showDraftToggle": false
        },
        "pages": {
            "ListReport|Workflows": {
                "entitySet": "Workflows",
                "component": {
                    "name": "sap.suite.ui.generic.template.ListReport",
                    "list": true,
                    "settings": {
                        "condensedTableLayout": true,
                        "smartVariantManagement": true,
                        "enableTableFilterInPageVariant": true,
                        "tableSettings": {
                            "type": "GridTable",
                            "multiSelect": true,
                            "selectAll": true,
                            "selectionLimit": 200
                        }
                    }
                },
                "pages": {
                    "ObjectPage|Workflows": {
                        "entitySet": "Workflows",
                        "defaultLayoutTypeIfExternalNavigation": "MidColumnFullScreen",
                        "component": {
                            "name": "sap.suite.ui.generic.template.ObjectPage"
                        }
                    }
                }
            }
        }
    },
    "sap.platform.hcp": {
        "uri": ""
    },
    "sap.cloud": {
        "public": true,
        "service": "a2aAgents"
    },
    "sap.app/ui5/cors": {
        "allowedMethods": ["GET", "POST", "PUT", "DELETE"],
        "allowedOrigins": ["https://*.sap.com"],
        "allowedHeaders": ["X-CSRF-Token", "Content-Type", "Authorization"],
        "exposeHeaders": ["X-CSRF-Token"],
        "maxAge": 3600
    },
    "sap.app/settings": {
        "securityHeaders": {
            "X-Frame-Options": "DENY",
            "X-XSS-Protection": "1; mode=block",
            "X-Content-Type-Options": "nosniff",
            "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://sapui5.hana.ondemand.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; connect-src 'self' https://localhost:8015 wss://localhost:8015; font-src 'self' data:; frame-ancestors 'none'",
            "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
            "Referrer-Policy": "strict-origin-when-cross-origin",
            "Permissions-Policy": "geolocation=(), microphone=(), camera=()"
        }
    },
    "sap.app/crossNavigation": {
        "inbounds": {
            "a2a-network-agent15-display": {
                "semanticObject": "OrchestratorAgent",
                "action": "display",
                "title": "Orchestrator Agent",
                "icon": "sap-icon://workflow-tasks",
                "signature": {
                    "parameters": {},
                    "additionalParameters": "allowed"
                }
            }
        }
    }
}