{
    "_version": "1.60.0",
    "sap.app": {
        "id": "a2a.network.agent15",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{agent15Title}}",
        "description": "{{agent15Description}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "mainService": {
                "uri": "https://a2a/agent15/v1/odata/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.fe.templates": {},
                "sap.f": {},
                "sap.m": {},
                "sap.ui.core": {},
                "sap.uxap": {},
                "sap.viz": {}
            }
        },
        "models": {
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "routing": {
            "routes": [
                {
                    "name": "DeploymentList",
                    "pattern": ":?query:",
                    "target": {
                        "type": "Component",
                        "name": "DeploymentList",
                        "options": {
                            "settings": {
                                "entitySet": "DeploymentTasks",
                                "variantManagement": "Page",
                                "initialLoad": true,
                                "controlConfiguration": {
                                    "@com.sap.vocabularies.UI.v1.LineItem": {
                                        "tableSettings": {
                                            "type": "ResponsiveTable",
                                            "selectionMode": "Multi"
                                        },
                                        "actions": {
                                            "DeployPackage": {
                                                "press": ".extension.a2a.network.agent15.ext.controller.ListReportExt.onDeployPackage",
                                                "visible": true,
                                                "enabled": true,
                                                "requiresSelection": true,
                                                "text": "Deploy Package"
                                            },
                                            "ConfigurePipeline": {
                                                "press": ".extension.a2a.network.agent15.ext.controller.ListReportExt.onConfigurePipeline",
                                                "visible": true,
                                                "enabled": true,
                                                "requiresSelection": false,
                                                "text": "Configure Pipeline"
                                            },
                                            "ViewDeploymentStatus": {
                                                "press": ".extension.a2a.network.agent15.ext.controller.ListReportExt.onViewDeploymentStatus",
                                                "visible": true,
                                                "enabled": true,
                                                "requiresSelection": false,
                                                "text": "View Deployment Status"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                {
                    "name": "DeploymentDetails",
                    "pattern": "DeploymentTasks({key}):?query:",
                    "target": {
                        "type": "Component",
                        "name": "DeploymentDetails",
                        "options": {
                            "settings": {
                                "entitySet": "DeploymentTasks",
                                "controlConfiguration": {
                                    "@com.sap.vocabularies.UI.v1.HeaderInfo": {
                                        "actions": {
                                            "RollbackDeployment": {
                                                "press": ".extension.a2a.network.agent15.ext.controller.ObjectPageExt.onRollbackDeployment",
                                                "visible": "{= ${status} === 'DEPLOYED' || ${status} === 'FAILED' }",
                                                "enabled": true,
                                                "text": "Rollback Deployment"
                                            },
                                            "ValidateDeployment": {
                                                "press": ".extension.a2a.network.agent15.ext.controller.ObjectPageExt.onValidateDeployment",
                                                "visible": true,
                                                "enabled": true,
                                                "text": "Validate Deployment"
                                            },
                                            "ViewLogs": {
                                                "press": ".extension.a2a.network.agent15.ext.controller.ObjectPageExt.onViewLogs",
                                                "visible": true,
                                                "enabled": true,
                                                "text": "View Deployment Logs"
                                            },
                                            "PromoteToProduction": {
                                                "press": ".extension.a2a.network.agent15.ext.controller.ObjectPageExt.onPromoteToProduction",
                                                "visible": "{= ${environment} !== 'PRODUCTION' && ${status} === 'DEPLOYED' }",
                                                "enabled": true,
                                                "text": "Promote to Production"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ListReport.ListReportController": {
                        "controllerName": "a2a.network.agent15.ext.controller.ListReportExt"
                    },
                    "sap.fe.templates.ObjectPage.ObjectPageController": {
                        "controllerName": "a2a.network.agent15.ext.controller.ObjectPageExt"
                    }
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": ["F0001A15"],
        "archeType": "transactional"
    },
    "sap.fe": {
        "template": "sap.fe.templates.ListReport.ObjectPage"
    },
    "sap.platform.cf": {
        "oAuthScopes": [
            "$XSAPPNAME.deployment.user",
            "$XSAPPNAME.deployment.admin"
        ]
    },
    "sap.security": {
        "headers": {
            "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self' wss:; font-src 'self' data:;",
            "X-Content-Type-Options": "nosniff",
            "X-Frame-Options": "DENY",
            "X-XSS-Protection": "1; mode=block",
            "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
            "Referrer-Policy": "strict-origin-when-cross-origin",
            "Permissions-Policy": "geolocation=(), microphone=(), camera=()"
        },
        "csrf": {
            "token": true
        },
        "cors": {
            "allowedOrigins": ["https://*.ondemand.com"],
            "allowedMethods": ["GET", "POST", "PUT", "DELETE"],
            "allowedHeaders": ["Content-Type", "Authorization", "X-CSRF-Token"],
            "exposeHeaders": ["X-CSRF-Token"],
            "maxAge": 3600
        }
    }
}