{
    "_version": "1.49.0",
    "sap.app": {
        "id": "a2a.network.agent11.ext",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "crossNavigation": {
            "inbounds": {
                "agent11-display": {
                    "semanticObject": "SQLAgent",
                    "action": "display",
                    "title": "{{agent.name}}",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/ZA2A_AGENT11_SRV/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://database",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.f": {},
                "sap.suite.ui.commons": {},
                "sap.suite.ui.microchart": {},
                "sap.viz": {},
                "sap.ui.table": {},
                "sap.ui.codeeditor": {},
                "sap.ui.unified": {},
                "sap.base": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "flexEnabled": false,
        "handleValidation": true,
        "models": {
            "i18n": {
                "dataSource": "i18n",
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "a2a.network.agent11.ext.i18n.i18n",
                    "supportedLocales": ["en", "de", "fr", "es"],
                    "fallbackLocale": "en"
                }
            },
            "": {
                "dataSource": "mainService",
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Request",
                    "refreshAfterChange": false,
                    "metadataUrlParams": {
                        "sap-value-list": "none"
                    },
                    "headers": {
                        "X-Requested-With": "XMLHttpRequest",
                        "X-Content-Type-Options": "nosniff",
                        "X-Frame-Options": "DENY",
                        "X-XSS-Protection": "1; mode=block",
                        "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
                        "Cache-Control": "no-store, no-cache, must-revalidate",
                        "Pragma": "no-cache"
                    }
                }
            }
        }
    },
    "sap.fe": {
        "type": "extension"
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.platform.abap": {
        "uri": "/sap/bc/ui5_ui5/sap/za2a_agent11/"
    },
    "sap.platform.hcp": {
        "uri": ""
    },
    "sap.cloud": {
        "public": false,
        "service": "a2aAgents"
    },
    "security": {
        "contentSecurityPolicy": {
            "policy": "default-src 'self'; script-src 'self' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; connect-src 'self'; font-src 'self'; object-src 'none'; media-src 'none'; child-src 'none';",
            "reportOnly": false
        },
        "frameOptions": "DENY",
        "contentTypeOptions": "nosniff",
        "downloadOptions": "noopen",
        "crossOriginEmbedderPolicy": "require-corp",
        "crossOriginOpenerPolicy": "same-origin",
        "crossOriginResourcePolicy": "same-origin",
        "sqlSecurity": {
            "maxQueryComplexity": 50,
            "maxExecutionsPerHour": 100,
            "allowedOperations": ["SELECT", "COUNT", "SHOW", "DESCRIBE", "EXPLAIN"],
            "auditLogging": true,
            "rateLimiting": true,
            "sqlInjectionPrevention": true,
            "parameterValidation": true
        }
    }
}