{
    "_version": "1.49.0",
    "sap.app": {
        "id": "a2a.network.agent14.ext",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "AI-ML-EMB",
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/sap/EMBEDDING_FINETUNE_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "i18n": {
                "uri": "i18n/i18n.properties",
                "type": "properties"
            }
        },
        "crossNavigation": {
            "inbounds": {
                "embeddingFineTuner": {
                    "semanticObject": "EmbeddingFineTuner",
                    "action": "manage",
                    "title": "{{appTitle}}",
                    "subTitle": "{{appDescription}}"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://machine-learning",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ui.layout": {},
                "sap.f": {},
                "sap.suite.ui.commons": {},
                "sap.suite.ui.microchart": {},
                "sap.viz": {},
                "sap.ui.table": {},
                "sap.ui.unified": {},
                "sap.ui.codeeditor": {}
            }
        },
        "models": {
            "i18n": {
                "dataSource": "i18n",
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "a2a.network.agent14.ext.i18n.i18n",
                    "supportedLocales": [""],
                    "fallbackLocale": ""
                }
            },
            "": {
                "dataSource": "mainService",
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true,
                    "preliminaryContext": true,
                    "headers": {
                        "X-Requested-With": "XMLHttpRequest"
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "flexEnabled": true,
        "handleValidation": true
    },
    "sap.fe": {
        "type": "extension"
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.cloud": {
        "public": true,
        "service": "a2aAgents"
    },
    "sap.app/ui5/cors": {
        "allowedMethods": ["GET", "POST", "PUT", "DELETE"],
        "allowedOrigins": ["https://*.sap.com"],
        "allowedHeaders": ["X-CSRF-Token", "Content-Type", "Authorization"],
        "exposeHeaders": ["X-CSRF-Token"],
        "maxAge": 3600
    },
    "sap.app/settings": {
        "securityHeaders": {
            "X-Frame-Options": "DENY",
            "X-XSS-Protection": "1; mode=block",
            "X-Content-Type-Options": "nosniff",
            "Content-Security-Policy": "default-src 'self'; script-src 'self' https://sapui5.hana.ondemand.com; style-src 'self' 'unsafe-inline' https://sapui5.hana.ondemand.com; img-src 'self' data: https:; connect-src 'self' https://localhost:8014 wss://localhost:8014 https://*.sap.com; font-src 'self' data: https://sapui5.hana.ondemand.com; frame-ancestors 'none'; object-src 'none'; base-uri 'self'; form-action 'self'; media-src 'none'; worker-src 'none'",
            "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
            "Referrer-Policy": "strict-origin-when-cross-origin",
            "Permissions-Policy": "geolocation=(), microphone=(), camera=(), payment=(), usb=(), serial=(), bluetooth=(), magnetometer=(), gyroscope=(), accelerometer=(), ambient-light-sensor=()",
            "Cross-Origin-Embedder-Policy": "require-corp",
            "Cross-Origin-Opener-Policy": "same-origin",
            "Cross-Origin-Resource-Policy": "same-origin",
            "Cache-Control": "no-store, must-revalidate",
            "Pragma": "no-cache",
            "X-Permitted-Cross-Domain-Policies": "none",
            "Feature-Policy": "autoplay 'none'; camera 'none'; microphone 'none'; payment 'none'; usb 'none'",
            "X-DNS-Prefetch-Control": "off"
        },
        "mlSecurity": {
            "maxModelSize": "2GB",
            "allowedModelFormats": ["pytorch", "tensorflow", "onnx", "safetensors"],
            "maxTrainingTime": 86400,
            "maxBatchSize": 1024,
            "maxEpochs": 1000,
            "enableAuditLogging": true,
            "requireModelIntegrityCheck": true,
            "blockSuspiciousPatterns": true,
            "maxRiskScore": 75
        }
    }
}