{
  "defaultRenderer": "fiori2",
  "renderers": {
    "fiori2": {
      "componentData": {
        "config": {
          "search": "hidden",
          "enableSearch": false,
          "rootIntent": "A2ANetwork-manage",
          "enableRecentActivity": true,
          "enablePersonalization": true,
          "enableTagFiltering": false,
          "enableCatalogTagFilter": false,
          "enableAbout": true,
          "enableUserDefaultParameters": true,
          "disableSortedLockedGroups": false,
          "enableTileActionsIcon": true,
          "enableSetTheme": true,
          "enableAccessibility": true,
          "enableHelp": true,
          "preloadLibrariesForRootIntent": false,
          "applications": {
            "A2ANetwork-manage": {
              "additionalInformation": "SAPUI5.Component=a2a.network.fiori",
              "applicationType": "URL",
              "url": "/a2a-fiori/webapp/",
              "navigationMode": "embedded"
            }
          }
        }
      }
    }
  },
  "services": {
    "LaunchPage": {
      "adapter": {
        "config": {
          "groups": [
            {
              "id": "a2a_network_group",
              "title": "A2A Network Applications",
              "isPreset": true,
              "isVisible": true,
              "isGroupLocked": false,
              "tiles": [
                {
                  "id": "overview_dashboard",
                  "tileType": "sap.ushell.ui.tile.DynamicTile",
                  "properties": {
                    "title": "Overview Dashboard",
                    "subtitle": "Network Overview & Status",
                    "icon": "sap-icon://home",
                    "targetURL": "#A2ANetwork-manage&/home",
                    "serviceUrl": "/api/v1/NetworkStats?id=overview_dashboard",
                    "serviceRefreshInterval": 30
                  }
                },
                {
                  "id": "blockchain_dashboard",
                  "tileType": "sap.ushell.ui.tile.DynamicTile",
                  "properties": {
                    "title": "Blockchain Dashboard",
                    "subtitle": "Distributed Ledger Monitoring",
                    "icon": "sap-icon://chain-link",
                    "targetURL": "#A2ANetwork-manage&/blockchain",
                    "serviceUrl": "/api/v1/blockchain/stats?id=blockchain_dashboard",
                    "serviceRefreshInterval": 30
                  }
                },
                {
                  "id": "agent_visualization",
                  "tileType": "sap.ushell.ui.tile.DynamicTile",
                  "properties": {
                    "title": "Agent Visualization",
                    "subtitle": "AI Agent Network Topology",
                    "icon": "sap-icon://network",
                    "targetURL": "#A2ANetwork-manage&/agents",
                    "serviceUrl": "/api/v1/Agents?id=agent_visualization",
                    "serviceRefreshInterval": 30
                  }
                },
                {
                  "id": "service_marketplace",
                  "tileType": "sap.ushell.ui.tile.DynamicTile",
                  "properties": {
                    "title": "Service Marketplace",
                    "subtitle": "AI Services & Capabilities",
                    "icon": "sap-icon://retail-store",
                    "targetURL": "#A2ANetwork-manage&/marketplace",
                    "serviceUrl": "/api/v1/Services?id=service_marketplace",
                    "serviceRefreshInterval": 30
                  }
                },
                {
                  "id": "network_health",
                  "tileType": "sap.ushell.ui.tile.DynamicTile",
                  "properties": {
                    "title": "Network Health",
                    "subtitle": "System Status & Performance",
                    "icon": "sap-icon://heart",
                    "targetURL": "#A2ANetwork-manage&/analytics",
                    "serviceUrl": "/api/v1/network/health",
                    "serviceRefreshInterval": 15
                  }
                },
                {
                  "id": "notification_center",
                  "tileType": "sap.ushell.ui.tile.DynamicTile",
                  "properties": {
                    "title": "Notifications",
                    "subtitle": "Alerts & Messages",
                    "icon": "sap-icon://bell",
                    "targetURL": "#A2ANetwork-manage&/alerts",
                    "serviceUrl": "/api/v1/notifications/count",
                    "serviceRefreshInterval": 10
                  }
                }
              ]
            }
          ],
          "catalogs": [
            {
              "id": "a2a_network_catalog",
              "title": "A2A Network Catalog",
              "tiles": [
                {
                  "id": "operations_tile",
                  "tileType": "sap.ushell.ui.tile.StaticTile",
                  "properties": {
                    "title": "Operations",
                    "subtitle": "System Operations & Monitoring",
                    "icon": "sap-icon://monitor-payments",
                    "targetURL": "#A2ANetwork-manage&/operations"
                  }
                },
                {
                  "id": "transactions_tile", 
                  "tileType": "sap.ushell.ui.tile.StaticTile",
                  "properties": {
                    "title": "Transactions",
                    "subtitle": "Transaction History & Analytics",
                    "icon": "sap-icon://transaction",
                    "targetURL": "#A2ANetwork-manage&/transactions"
                  }
                },
                {
                  "id": "settings_tile",
                  "tileType": "sap.ushell.ui.tile.StaticTile",
                  "properties": {
                    "title": "Settings",
                    "subtitle": "System Configuration",
                    "icon": "sap-icon://settings",
                    "targetURL": "#A2ANetwork-manage&/settings"
                  }
                }
              ]
            }
          ]
        }
      }
    },
    "NavTargetResolution": {
      "adapter": {
        "config": {
          "applications": {
            "A2ANetwork-manage": {
              "sap.app/id": "a2a.network.fiori",
              "sap.app/applicationVersion": {
                "version": "1.0.0"
              },
              "sap.app/title": "A2A Network",
              "sap.app/subTitle": "Enterprise Agent Network Management",
              "sap.app/shortTitle": "A2A Network",
              "sap.app/info": "Enterprise Autonomous Agent Orchestration Platform",
              "sap.app/description": "Comprehensive platform for managing autonomous agents, blockchain integration, and enterprise workflows",
              "sap.app/tags": {
                "keywords": ["autonomous", "agents", "blockchain", "enterprise", "orchestration"]
              },
              "sap.ui/technology": "UI5",
              "sap.ui5/componentName": "a2a.network.fiori",
              "sap.platform.abap/uri": "/a2a-fiori/webapp/",
              "sap.flp": {
                "type": "application"
              },
              "sap.ui/fullWidth": true
            }
          }
        }
      }
    },
    "ClientSideTargetResolution": {
      "adapter": {
        "config": {
          "applications": {
            "A2ANetwork-manage": {
              "sap.app/id": "a2a.network.fiori",
              "sap.app/applicationVersion": {
                "version": "1.0.0"
              },
              "sap.flp": {
                "type": "application"
              }
            }
          }
        }
      }
    }
  },
  "bootstrapPlugins": {
    "RuntimeAuthoringPlugin": {
      "component": "sap.ushell.plugins.rta",
      "config": {
        "validateAppVersion": false
      }
    },
    "PersonalizePlugin": {
      "component": "sap.ushell.plugins.rta-personalize",
      "config": {
        "validateAppVersion": false
      }
    }
  }
}