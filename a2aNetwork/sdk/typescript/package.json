{
  "name": "@a2a-network/typescript-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for A2A Network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "a2a",
    "network",
    "blockchain",
    "agent",
    "sdk"
  ],
  "author": "A2A Network Team",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.9.0",
    "ws": "^8.16.0",
    "events": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "prettier": "^3.1.1"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/a2a-network/sdk"
  }
}