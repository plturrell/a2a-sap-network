# A2A Network SDK Configuration

# Ethereum Network Configuration
INFURA_PROJECT_ID=your_infura_project_id_here
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Custom RPC URLs (optional - will use Infura/Alchemy if not provided)
MAINNET_RPC_URL=
SEPOLIA_RPC_URL=
POLYGON_RPC_URL=

# Default Network (localhost, mainnet, sepolia, polygon)
DEFAULT_NETWORK=localhost

# Private Keys (for testing - NEVER use real private keys in production)
PRIVATE_KEY=your_private_key_for_testing
AGENT_PRIVATE_KEY=your_agent_private_key_for_testing

# WebSocket Configuration
WEBSOCKET_URL=ws://localhost:8545

# API Configuration
API_TIMEOUT=30000
RETRY_ATTEMPTS=3
AUTO_RECONNECT=true

# Logging Configuration
LOG_LEVEL=info
LOG_TO_FILE=false
LOG_FILE_PATH=./logs/a2a-sdk.log

# Development Settings
DEBUG_MODE=false
ENABLE_METRICS=true
CACHE_ENABLED=true

# Contract Addresses (will be populated when contracts are deployed)
MAINNET_AGENT_REGISTRY=
MAINNET_MESSAGE_ROUTER=
MAINNET_TOKEN_MANAGER=
MAINNET_GOVERNANCE=

SEPOLIA_AGENT_REGISTRY=
SEPOLIA_MESSAGE_ROUTER=
SEPOLIA_TOKEN_MANAGER=
SEPOLIA_GOVERNANCE=

POLYGON_AGENT_REGISTRY=
POLYGON_MESSAGE_ROUTER=
POLYGON_TOKEN_MANAGER=
POLYGON_GOVERNANCE=